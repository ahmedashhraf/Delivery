<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#f3e500" android:endColor="#00bed6" android:angle="180" android:type="linear"/>
            <corners android:topLeftRadius="@dimen/dp_22" android:topRightRadius="@dimen/dp_22" android:bottomLeftRadius="@dimen/dp_22" android:bottomRightRadius="@dimen/dp_0"/>
