    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#fff"/>
            <corners android:topLeftRadius="@dimen/dp_22" android:topRightRadius="@dimen/dp_22" android:bottomLeftRadius="@dimen/dp_22" android:bottomRightRadius="@dimen/dp_0"/>
        </shape>
    </item>
