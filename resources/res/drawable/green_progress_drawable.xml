<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="0dp"/>
            <gradient android:startColor="#881dcad3" android:endColor="#881dcad3" android:angle="270" android:centerY="0.75" android:centerColor="#881dcad3"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="0dp"/>
                <gradient android:startColor="#881dcad3" android:endColor="#881dcad3" android:angle="270" android:centerY="0.75" android:centerColor="#881dcad3"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0dp"/>
                <gradient android:startColor="@color/colorPrimary" android:endColor="@color/colorPrimary" android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>
