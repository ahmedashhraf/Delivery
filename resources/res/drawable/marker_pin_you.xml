<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@+id/shapwGradient">
        <shape android:id="@+id/shap1" android:shape="rectangle">
            <gradient android:id="@+id/GR" android:startColor="#f3e500" android:endColor="#00bed6" android:angle="360" android:type="linear" android:gradientRadius="360"/>
            <corners android:radius="@dimen/dp_30"/>
        </shape>
    </item>
    <item android:id="@+id/shapwWhite" android:left="@dimen/dp_2" android:top="@dimen/dp_2" android:right="@dimen/dp_2" android:bottom="@dimen/dp_2">
        <shape android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/dp_30"/>
        </shape>
    </item>
</layer-list>
